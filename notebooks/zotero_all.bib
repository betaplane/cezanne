@article{tziperman_irregularity_1995,
	title = {Irregularity and {Locking} to the {Seasonal} {Cycle} in an {ENSO} {Prediction} {Model} as {Explained} by the {Quasi}-{Periodicity} {Route} to {Chaos}.},
	volume = {52},
	url = {http://adsabs.harvard.edu/cgi-bin/nph-data_query?bibcode=1995JAtS...52..293T&link_type=ABSTRACT},
	doi = {10.1175/1520-0469(1995)052<0293:IALTTS>2.0.CO;2},
	abstract = {The behavior of the Cane-Zebiak ENSO prediction model is analyzed as a function of model parameters measuring the strength of coupling between the model ocean and atmosphere and the amplitude of the back-ground seasonal cycle specified in the model. As either of these two parameters is increased, the model undergoes a transition from periodic to chaotic behavior according to the universal quasi-periodicity route to chaos. Thus, the irregularity of model ENSO events and their partial locking to the seasonal cycle can both be explained as low-order chaotic behavior driven by the seasonal cycle. The chaos is due to irregular jumping of the Pacific natural ocean-atmosphere oscillator between different nonlinear resonances with the seasonal forcing.The periodic seasonal forcing seems to be the main factor determining the chaotic behavior of the model. However, the full irregularity of model ENSO events is only explained by considering additional factors, possibly including the nonlinear interaction of different delay oscillator modes related to the different model ocean Rossby modes.},
	number = {3},
	journal = {Journal of Atmospheric Sciences},
	author = {Tziperman, Eli and Cane, Mark A and Zebiak, Stephen E},
	month = feb,
	year = {1995},
	pages = {293--306},
	file = {Tziperman 1995 Journal of Atmospheric Sciences:/home/arno/.zotero/zotero/4a0hlybs.default/zotero/storage/493BZ4V9/Tziperman 1995 Journal of Atmospheric Sciences.pdf:application/pdf}
}

