@article{schopf_vacillations_1988,
	title = {Vacillations in a {Coupled} {Ocean}‚Äì{Atmosphere} {Model}},
	url = {http://journals.ametsoc.org/doi/abs/10.1175/1520-0469(1988)045%3C0549:VIACOM%3E2.0.CO;2},
	abstract = {Abstract Results are presented from a 35-year integration of a coupled ocean-atmosphere model. Both ocean and atmosphere are two-level, nonlinear primitive equations models. The global atmospheric model is forced by a steady, zonally symmetric Newtonian heating. The ocean model is solved in a rectangular tropical basin. Heat fluxes between ocean and atmosphere are linear in air-sea temperature differences, and the interfacial stress is proportional to lower-level atmospheric winds. The coupled models produce ENSO-like variability on time scales of 3 to 5 years. Since there is no external time-dependent forcing, these are self-sustained vacillations of the nonlinear system. It is argued that the energetics of the vacillations is that of unstable coupled modes and that the time scale is crucially dependent on the effects of ocean waves propagating in a closed basin.},
	journal = {http://dx.doi.org/10.1175/1520-0469(1988)045{\textless}0549:VIACOM{\textgreater}2.0.CO;2},
	author = {Schopf, Paul S and Suarez, Max J},
	month = jan,
	year = {1988},
	file = {Schopf 1988 httpdx.doi.org10.11751520-0469(1988)0450549VIACOM2.0.CO2:/home/arno/.zotero/zotero/4a0hlybs.default/zotero/storage/CG5UIIIC/Schopf 1988 httpdx.doi.org10.11751520-0469(1988)0450549VIACOM2.0.CO2.pdf:application/pdf}
